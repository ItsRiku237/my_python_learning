Name = "Jnv Sonepur"
short_name = Name[0:3] # include initial index and end index is not include
s1 = Name[4]
s2 = Name[:3]
s3 = Name[4:]


print(short_name)
print(s1)
print(s2)
print(s3)
print(Name[-7:-1])

# SLICING WITH SKIP VALUE
s4 = "Jawahar Navodaya vidyalaya"
# s4 [3 : 14: 2]
print(s4 [3 : 14: 2])
